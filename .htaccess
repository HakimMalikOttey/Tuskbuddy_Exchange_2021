
RewriteEngine on
RewriteCond %{HTTP_HOST} ^tuskbuddy\.com$ [OR]
RewriteCond %{HTTP_HOST} ^www\.tuskbuddy\.com$
RewriteRule ^/?$ "http\:\/\/townseed\.ca\/tuskbuddy\/" [R=301,L]
RewriteCond %{THE_REQUEST} /([^.]+)\.html [NC]
RewriteRule ^ /%1 [NC,L,R]

RewriteCond %{REQUEST_FILENAME}.html -f
RewriteRule ^ %{REQUEST_URI}.html [NC,L]

